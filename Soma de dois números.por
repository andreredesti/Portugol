programa
{

	//Algoritmo "Exemplo 1"

	real = n1, n2, soma

	funcao inicio()
	
	
	{
		escreva("Digite um número ")
		leia (n1);
		escreva ("Digite um outro número ")
		leia (n2)
		soma = n1 + n2
		escreva ("\nO primeiro númmero digitado é = ", n1)
		escreva ("\nO segundo número digitado foi = ", n2)
		escreva ("\nA soma dos números é ", soma)
		
	}
}

